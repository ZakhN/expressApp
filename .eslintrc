{
  "extends": "airbnb-base",
  "env": {
    "es6": false,
    "node": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "semi": ["error", "always"],
    "quotes": ["error", "single"],
    "linebreak-style": ["error", "unix"],
    "indent": ["error", 2],
    "object-curly-spacing": ["error", "always"]
  }
}